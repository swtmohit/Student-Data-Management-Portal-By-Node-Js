    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: "Gupter", serif;
        }

        .headLogo img {
            width: 100%;
            height: 70px;
        }

        .navbarData {
            font-size: 30px;
            font-family: "Oswald", sans-serif;
        }

        .sideBar {
            width: 256px;
            height: 89vh;
            font-family: "Kalnia Glaze", serif;

        }

        .DecorationFor {
            background-color: rgb(38, 13, 44);
            /* font-size: 20px; */
        }

        .image {
            box-shadow: 1px 1px 20px rgb(179, 247, 227);
        }

        .developerheading {
            text-shadow: 12px 10px 3px rgb(179, 247, 227);
            font-weight: 600;
        }

        .cardHover:hover {
            background-color: rgb(179, 247, 227);
            transition: linear 0.2s;
        }

        .img-container {
            overflow: hidden;
        }

        .card-img-top {
            transition: transform 0.3s ease;
            width: 100%;
        }

        .img-container:hover .card-img-top {
            transform: scale(1.2);
            /* Adjust the scale value as needed */
        }

        .shine-button {
            background: linear-gradient(90deg, #5adfee, #da1b60);
            border: none;
            color: white;
            padding: 10px 25px;
            font-size: 18px;
            font-weight: bold;
            border-radius: 20px;
            position: relative;
            cursor: pointer;
            overflow: hidden;
            transition: all 0.4s ease-in-out;
        }

        .shine-button::before {
            content: '';
            position: absolute;
            top: 50%;
            left: -100%;
            width: 100%;
            height: 300%;
            background: rgba(255, 255, 255, 0.4);
            transform: rotate(45deg);
            transition: all 0.5s ease-in-out;
        }

        .shine-button:hover::before {
            left: 100%;
        }

        .secondCard {
            box-shadow: 1px 1px 10px #ccc;
        }
        .optionBackground{
            background-image: url(img/button.jpeg);
        }
